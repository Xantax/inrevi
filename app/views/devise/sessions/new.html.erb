<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div><%= f.label :email %><br />
  <%= f.email_field :email, :class => "form-control autotitle", autofocus: true %></div>

  <div><%= f.label :password %><br />
    <%= f.password_field :password, :class => "form-control autotitle", autocomplete: "off" %></div><br>

  <%= f.submit "Sign in", class: "btn btn-large btn-primary", :onclick => "this.disabled = true" %><br><br>

  <% if devise_mapping.rememberable? %>
    <%= f.check_box :remember_me %> <%= f.label :remember_me %>
  <% end %>

<% end %>
