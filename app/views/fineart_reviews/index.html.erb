<h1>Listing fineart_reviews</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th>Fineart</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @fineart_reviews.each do |fineart_review| %>
      <tr>
        <td><%= fineart_review.content %></td>
        <td><%= fineart_review.user %></td>
        <td><%= fineart_review.fineart %></td>
        <td><%= link_to 'Show', fineart_review %></td>
        <td><%= link_to 'Edit', edit_fineart_review_path(fineart_review) %></td>
        <td><%= link_to 'Destroy', fineart_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fineart review', new_fineart_review_path %>
