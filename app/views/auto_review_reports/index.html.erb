<% @auto_review_reports.each do |auto_review_report| %><br><br>

 
<%= link_to "Original Review" , auto_auto_review_path(auto_id: auto_review_report.auto_review.auto.id, id: auto_review_report.auto_review.id) %>

<%= link_to user_path(auto_review_report.user) do %> 
   <%= auto_review_report.user.first_name %>
   <%= auto_review_report.user.last_name.truncate(2, omission: '.')%>
<% end %>

<%= auto_review_report.content %>

<% end %>
