    <%= form_tag autos_search_path, :method => :get do %>      
      <div class="searchbox">
        <%= text_field_tag :search, params[:search], :placeholder => "e.g. Tesla S..." , class: "form-control search" %>
      </div>
      <div class="searchgo">
        <span class="input-group-btn">
          <%= submit_tag "Go", class: "btn btn-default" %>
        </span>
      </div>
    <% end %>