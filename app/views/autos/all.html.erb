<% provide(:title, 'All Auto') %>

      <% @autos.each do |auto| %>
          <div class="row">
            <%= link_to auto.name, auto %><br>
            <%= image_tag auto.image_url(:thumb) if auto.image? %>  
            <hr>
         </div>
      <% end %>

<div class="digg_pagination">
  <%= will_paginate @autos %>
</div>