<h1>Listing podcast_sub_categories</h1>

<table>
  <thead>
    <tr>
      <th>Podcastcategory</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @podcast_sub_categories.each do |podcast_sub_category| %>
      <tr>
        <td><%= podcast_sub_category.name %></td>
        <td><%= podcast_sub_category.podcast_category.name %></td>
        <td><%= link_to 'Show', podcast_sub_category %></td>
        <td><%= link_to 'Edit', edit_podcast_sub_category_path(podcast_sub_category) %></td>
        <td><%= link_to 'Destroy', podcast_sub_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

