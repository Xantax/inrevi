<%= form_for @tech, validate: true, html: {multipart: true}, 'data-update-target' => 'comments', class: 'comments' do |f| %>

<%= render 'shared/main_error_messages', object: f.object %>

    <%= f.label :name %><br>
    <%= f.text_field :name, :class => "form-control autotitle" %><br><br>

    <%= f.label :tag_list, "Category - separate by commas" %><br>
    <%= f.text_field :tag_list, :placeholder => "e.g hosting, accounting" , :class => "form-control autotitle" %><br><br>

    <%= f.label :website, "Website" %><br>
    <%= f.text_field :website, :class => "form-control autotitle", :placeholder => "include https://..." %><br><br>

    <%= image_tag("othericons/PhotoShow.PNG") %>
<%= f.file_field :image, :placeholder => "e.g. https://imgur.com/Iks3rd" , :class => "podcastnew" %><br><br>

    <%= submit_tag 'Submit', :class => 'btn btn-default', :id => "submit", data: { disable_with: "Uploading..." } %> <br>

<% end %>

<div class="row dumb300"></div>