<% provide(:title, 'All Tech') %>

      <% @teches.each do |tech| %>
          <div class="row">
            <%= link_to tech.name, tech %><br>
            <%= image_tag tech.image_url(:thumb) if tech.image? %>  
            <hr>
         </div>
      <% end %>

<div class="digg_pagination">
  <%= will_paginate @teches %>
</div>