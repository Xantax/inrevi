<% provide(:title, 'Apps, Services, Websites...') %>

<div class="row">

  <div class="col-sm-2 categories">
    <%= render 'shared/categories' %>
  </div>
    
 <div class="col-sm-8">   
    <div class="allnews">

          <%= form_tag teches_search_path, :method => :get do %>
      
              <div class="searchbox">
                <%= text_field_tag :search, params[:search], :placeholder => "by name" , :class => "form-control" %>
              </div>
      
              <div class="searchgo">
                <span class="input-group-btn">
                  <%= submit_tag "Go", class: "btn btn-default" %>
                </span>
              </div>
      
          <% end %>
          <hr>      
        <div class="row dumb500"></div>      
    </div>
 </div>
   
  <div class="col-sm-2">
    <%= render 'shared/user_mainprofile' %>
  </div>

</div>
