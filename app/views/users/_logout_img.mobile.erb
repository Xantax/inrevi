  <% if signed_in? %>

    <% if current_user?(@user) %>
      <div class="row">
        
          <div class="col-xs-6">
            <%= image_tag("othericons/PhotoShow.PNG", width: '14', height: '14') %>
              <%= link_to "change image", edit_user_path(@user) %>
          </div>
        
        <div class="col-xs-6">
          <%= image_tag('othericons/logout.PNG') %>
          <div class="logoutlink">
            <%= link_to "logout", destroy_user_session_path, :method => :delete %>
          </div>
        </div>

      </div>
    <% end %>

  <% end %>