<table class="table table-striped table-hover">
  <tr>
    <th>Title</th>
    <th>Email</th>
    <th>Created</th>
    <th></th>
    <th>Status</th>
  </tr>
 
  <% @contact_forms.each do |contact_form| %>
    <tr>
      <td><%= contact_form.category %></td>
      <td><%= contact_form.email %></td>
      <td><%= time_ago_in_words(contact_form.created_at) %></td>
      <td><%= link_to 'Show', contact_form_path(contact_form) %></td>
      <td><%= image_tag('othericons/checkbox_blue.PNG') if contact_form.read %></td>

    </tr>
  <% end %>
</table>

<div class="digg_pagination">
  <%= will_paginate @contact_forms %>
</div>
